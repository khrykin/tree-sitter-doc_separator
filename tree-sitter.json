{
  "grammars": [
    {
      "name": "doc_separator",
      "camelcase": "DocSeparator",
      "scope": "source.doc_separator",
      "file-types": [
        ".md"
      ],
      "injection-regex": "^doc_separator$"
    }
  ],
  "metadata": {
    "version": "0.1.0",
    "license": "MIT",
    "description": "A parser for separators in Markdown",
    "authors": [
      {
        "name": "Dmitry Khrykin",
        "email": "khrykin@me.com"
      }
    ],
    "links": {
      "repository": "https://github.com/tree-sitter/tree-sitter-doc_separator"
    }
  },
  "bindings": {
    "c": true,
    "go": true,
    "node": true,
    "python": true,
    "rust": true,
    "swift": true
  }
}